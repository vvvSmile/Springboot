<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.smile.springboot.mapper.MybatisPersonMapper">
    <select id="getPersons" resultType="cn.smile.springboot.model.MybatisPerson">
        <!--完整路径，也可以直接写别名，但是需要在配置文件中加type-aliases-package:-->
        select * from mybatis_person
    </select>
</mapper>
